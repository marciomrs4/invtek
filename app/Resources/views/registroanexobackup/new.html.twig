{% extends ':template:template.html.twig' %}


    {% block contentheader %}

        <section class="content-header">
            <h1>
                Registro Anexo Backup
                <small>Novo</small>
            </h1>
        </section>

    {% endblock %}


    {% block panelheading %}

        <a href="{{ path('cadastro_registroanexobackup_index',{ 'registroBackup': registroBackup.id }) }}" class="btn btn-primary btn-sm">
            <span class="fa fa-list"></span>
            Listar
        </a>
        <a href="{{ path('cadastro_registrobackup_show',{ 'id': registroBackup.id }) }}" class="btn btn-primary btn-sm">
            <span class="fa fa-backward"></span>
            Registro Backup
        </a>

    {% endblock %}


    {% block panelbody %}

        {{ form_start(form) }}
        {{ form_widget(form) }}

    {% endblock %}

    {% block panelfooter %}
        <button type="submit" class="btn btn-primary btn-sm">
            <span class="glyphicon glyphicon-floppy-disk"></span>
            Salvar
        </button>
        {{ form_end(form) }}

    {% endblock %}
