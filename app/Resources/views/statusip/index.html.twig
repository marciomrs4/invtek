    {% extends ':template:template.html.twig' %}


    {% block contentheader %}


    <section class="content-header">
        <h1>
            Status Ip
            <small>Listar</small>
        </h1>
    </section>


    {% endblock %}


    {% block panelheading %}
    <a href="{{ path('cadastro_statusip_new') }}" class="btn btn-primary btn-sm">
            <span class="glyphicon glyphicon-plus"></span>
            Novo
        </a>
    {% endblock %}

    {% block panelbody %}
    <div class="table-responsive">
        <table class="table table-bordered table-condensed table-hover table-striped">
            <thead>
            <tr>
                <th>Ações</th>
                    <th>Nome</th>
                    <th>Código</th>
            </tr>
            </thead>
            <tbody>
            {% for statusIp in statusIps %}
            <tr>
                <td>
                    <a href="{{ path('cadastro_statusip_show', { 'id': statusIp.id }) }}" class="btn btn-primary btn-sm">
                        <span class="glyphicon glyphicon-search"></span>
                    </a>
                    <a href="{{ path('cadastro_statusip_edit', { 'id': statusIp.id }) }}" class="btn btn-primary btn-sm">
                        <span class="glyphicon glyphicon-edit"></span>
                    </a>
                </td>
                <td><a href="{{ path('cadastro_statusip_show', { 'id': statusIp.id }) }}">{{ statusIp.nome }}</a></td>
                <td>{{ statusIp.id }}</td>
            </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>


    {% endblock %}
